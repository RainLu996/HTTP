<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>OpenAccount</servlet-name>
        <servlet-class>com.lujun61.cookie.OpenAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consume</servlet-name>
        <servlet-class>com.lujun61.cookie.Consume</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Consume</servlet-name>
        <url-pattern>/consume</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OpenAccount</servlet-name>
        <url-pattern>/open</url-pattern>
    </servlet-mapping>
</web-app>